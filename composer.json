{
    "name": "setrun/setrun-content",
    "description": "Content component for SETRUN CMS",
    "keywords": ["yii2", "setrun", "framework", "module", "component", "yii2-extension", "extension"],
    "homepage": "http://setrun.ru",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name" : "Alexey Bashkov",
            "email": "ab@smpage.ru",
            "role" : "Ui Ux Designer"
        },
        {
            "name" : "Denis Utkin",
            "email": "dizirator@gmail.com",
            "role" : "Developer",
            "homepage": "http://github.com/dizirator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "setrun/setrun-sys": "*",
        "yii2tech/ar-linkmany": "^1.0",
        "la-haute-societe/yii2-save-relations-behavior": "^1.2",
        "paulzi/yii2-auto-tree" : "^1.0",
        "paulzi/yii2-adjacency-list": "^2.0.3",
        "paulzi/yii2-nested-sets": "^1.0.4",
        "paulzi/yii2-nested-intervals": "^1.0.5",
        "paulzi/yii2-materialized-path": "^2.0.6"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "setrun\\content\\": "src/"
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
